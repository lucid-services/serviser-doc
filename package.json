{
  "name": "bi-service-doc",
  "version": "2.0.4",
  "bin": "./bin/bi-service-doc.js",
  "description": "automated API documentation module",
  "main": "index.js",
  "scripts": {
    "test:acceptance": "mocha --ui bdd --colors --check-leaks -t 5000 --reporter spec --parse-pos-args false  'tests/acceptance/**/*.js'",
    "test:unit": "mocha --ui bdd --colors --check-leaks -t 5000 --reporter spec  'tests/unit/**/*.js'",
    "test": "npm run test:unit && npm run test:acceptance"
  },
  "testFramework": "mocha",
  "keywords": [
    "documentation",
    "api",
    "bi-service",
    "service",
    "automatization",
    "autogeneration"
  ],
  "files": [
    "bin",
    "lib",
    "public",
    "index.js",
    "README.md",
    "CHANGELOG.md"
  ],
  "author": "Bohemia Interactive",
  "private": false,
  "peerDependencies": {
    "bi-service": "^1.0.0",
    "express": "^4.14.0"
  },
  "dependencies": {
    "bi-ajv2swagger": "^0.2.7",
    "json-schema-faker": "~0.4.7",
    "json5": "^1.0.1",
    "lodash": "^4.16.2",
    "mustache": "^2.3.0",
    "uuid": "^3.3.2",
    "yargs": "^8.0.0"
  },
  "license": "MIT",
  "devDependencies": {
    "bi-service": "^1.6.0",
    "express": "^4.14.0",
    "chai": "^3.5.0",
    "istanbul": "^0.4.3",
    "chai-as-promised": "^5.3.0",
    "sinon-chai": "^2.8.0",
    "sinon": "^1.17.3",
    "sinon-as-promised": "^4.0.0",
    "mocha": "^2.5.3"
  }
}
